<Button>:
    font_size: 40
    size_hint: .3, .1

<Label>:
    font_size: 40
    size: self.texture_size

<MeuLayout>:
    senha: senha
    Label:
        text: "Senha: "
        pos_hint: {"center_x": .3, "center_y": .525}
    Button:
        pos_hint: {"center_x": .5, "center_y": .4}
        text: "Confirmar"
        on_press: root.gerarSenha()